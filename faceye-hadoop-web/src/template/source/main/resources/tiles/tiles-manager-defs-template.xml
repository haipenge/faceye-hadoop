<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <!-- 模块:@component.name@ 业务对像:@entity.name@ 管理后台 tiles模板配置 -->
    <!-- 作者:haipege -->
    <!-- @entity.name@ 管理 首页(列表页) -->
	<definition name="@component.name@.@entity.config.name@.manager" extends="default.manager">
	    <put-attribute name="default-center" value="@component.name@.@entity.config.name@.center"></put-attribute>
	</definition>
	<definition name="@component.name@.@entity.config.name@.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/@component.name@/@entity.config.name@/@entity.config.name@_center.jsp"></put-attribute>
	</definition>
	
	<!-- @entity.name@ 新增,编辑页面 -->
	<definition name="@component.name@.@entity.config.name@.update" extends="default.manager">
	    <put-attribute name="default-center" value="@component.name@.@entity.config.name@.update.center"></put-attribute>
	</definition>
	<definition name="@component.name@.@entity.config.name@.update.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/@component.name@/@entity.config.name@/@entity.config.name@_update.jsp"></put-attribute>
	</definition>
	
	<!-- @entity.name@ 明细页 -->
	<definition name="@component.name@.@entity.config.name@.detail" extends="default.manager">
	    <put-attribute name="default-center" value="@component.name@.@entity.config.name@.detail.center"></put-attribute>
	</definition>
	<definition name="@component.name@.@entity.config.name@.detail.center" extends="default.center">
	    <put-attribute name="default-center-right" value="/component/@component.name@/@entity.config.name@/@entity.config.name@_detail.jsp"></put-attribute>
	</definition>
	
</tiles-definitions>
